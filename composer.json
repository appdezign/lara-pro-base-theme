{
    "name": "appdezign/lara-pro-base-theme",
    "description": "Lara Pro CMS Base Theme",
	"version": "10.0.1",
    "keywords": ["Lara", "CMS"],
    "license": "proprietary",
    "homepage": "https://firmaq.nl",
    "type": "laravel-library",
    "authors": [
        {
            "name": "Sybrand Hoeksma",
            "email": "s.hoeksma@firmaq.nl"
        }
    ],
    "require": {
		"php": "^8.0",
		"appdezign/lara-pro-cms": "^10.0",
		"appdezign/lara-pro-demo-theme": "^10.0",
		"composer/installers": "~1.0",
		"laravel/framework": "^11.0"
	},
	"autoload": {
		"psr-4": {
			"Laratheme\\Base\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"LaraBaseServiceProvider"
			],
			"dont-discover": [
				"appdezign/lara-pro-base-theme"

			]
		},
		"installer-paths": {
			"laracms/themes/{$name}": [
				"appdezign/lara-pro-base-theme"
			]
		}
	}
}
